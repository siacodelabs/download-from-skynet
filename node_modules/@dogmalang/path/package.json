{
  "name": "@dogmalang/path",
  "version": "0.1.0",
  "description": "Utilities for working with file and directory paths.",
  "homepage": "http://dogmalang.com",
  "author": {
    "name": "Justo Labs",
    "email": "hello@justolabs.com",
    "url": "http://justolabs.com"
  },
  "contributors": [
    {
      "name": "Raúl G. González",
      "email": "raulggonzalez@justolabs.com",
      "url": ""
    }
  ],
  "keywords": [
    "fs",
    "file system",
    "Dogma"
  ],
  "bugs": {
    "url": "https://github.com/dogmalang/path/issues",
    "email": "issues@dogmalang.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dogmalang/path.git"
  },
  "preferGlobal": false,
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "engines": {
    "node": ">= 8.0",
    "npm": ">= 3.10"
  },
  "dependencies": {
    "@dogmalang/core": "*"
  },
  "devDependencies": {
    "@babel/cli": "*",
    "@babel/core": "*",
    "@babel/preset-env": "*",
    "eslint": ">= 4.11",
    "justo": ">= 1.0.0-alpha2.0",
    "justo.assert": ">= 1.0.0-alpha2.0",
    "justo.spy": ">= 1.0.0-alpha2.0",
    "justo.plugin.babel": ">= 1.0.0-alpha2.0",
    "justo.plugin.cli": ">= 1.0.0-alpha2.0",
    "justo.plugin.eslint": ">= 1.0.0-alpha2.0",
    "justo.plugin.fs": ">= 1.0.0-alpha2.0",
    "justo.plugin.npm": ">= 1.0.0-alpha2.0",
    "nyc": ">= 11.3"
  },
  "scripts": {
    "test": "justojs r test",
    "cov": "justojs r make && node_modules/.bin/nyc justojs r test",
    "pipelines": "node_modules/.bin/nyc justojs r test"
  },
  "nyc": {
    "check-coverage": true,
    "cache": true,
    "exclude": [
      "Justo.*.js",
      "**/test/**/*.js"
    ],
    "lines": 100,
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
